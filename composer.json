{
  "name": "spryker/company-business-unit-sales-connector",
  "type": "library",
  "description": "CompanyBusinessUnitSalesConnector module",
  "license": "proprietary",
  "require": {
    "php": ">=7.2",
    "spryker/company-business-unit": "^2.11.0",
    "spryker/company-sales-connector": "^1.0.0",
    "spryker/kernel": "^3.33.0",
    "spryker/permission-extension": "^1.0.0",
    "spryker/sales": "^11.5.0",
    "spryker/sales-extension": "^1.3.0",
    "spryker/uuid-behavior": "^1.0.0",
    "spryker/zed-request": "*"
  },
  "require-dev": {
    "spryker/code-sniffer": "*",
    "spryker/testify": "*"
  },
  "autoload": {
    "psr-4": {
      "Spryker\\": "src/Spryker/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/",
    "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/SprykerStrict/ruleset.xml src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "sort-packages": true
  }
}
